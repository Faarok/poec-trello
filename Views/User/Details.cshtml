<div>
    <ul>
        <li>ID : @Model.ID</li>
        <li>Name : @Model.Name</li>
        <li>Email : @Model.Email</li>
        <li>Signup date : @Model.SignupDate.ToString("yyyy/MM/dd")</li>
        <li><a asp-controller="User" asp-action="EditUser" asp-route-id="@Model.ID">Edit</a></li>
        <li><a asp-controller="User" asp-action="DeleteUser" asp-route-id="@Model.ID">Delete</a></li>
    </ul>
</div>